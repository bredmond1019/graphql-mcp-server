# Development environment variables
# Copy this file to .env.development and add your actual API key

# Required - Healthie API configuration
HEALTHIE_API_URL=https://staging-api.gethealthie.com/graphql
HEALTHIE_API_KEY=<your-actual-api-key>

# Optional configuration
LOG_LEVEL=INFO
DEBUG_MODE=false
SCHEMA_DIR=./schemas
CACHE_ENABLED=true
CACHE_DURATION_HOURS=24
REQUEST_TIMEOUT=30
MAX_RETRIES=3

# Additional tools configuration
# Set to true to enable 9 additional development tools (17 total instead of 8)
HEALTHIE_ENABLE_ADDITIONAL_TOOLS=false

# Note: .env.development is included in .gitignore and will not be committed